
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>

   

    <script src="~/Template/js/vendor/jquery-1.12.4.min.js"></script>
    @RenderSection("scripts", required: false)
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width" />
    <title class="text-center">پنل مدیریت ادمین</title>
</head>
<body>
   
    <div class="text-center"style="background-color:#f6eeee">
<img src="~/Img/Logo/GamLogo.png" style="height:150px" /></div>
    <div>
        @for (int i = 0; i <1; i++)
        {
            <br />
        }
        <div class="text-center">
            
            <div style="margin:auto;font-family:Tahoma">


                <style>
                    .a {
                        font-size: medium;
                        border-color:#000000;
                        background-color:#808080
                    }
                    .logoutbtnstyle{
                          border-color:#808080;
                        background-color:#ff6a00;
                    }
                </style>
           
                
                <a href="/AdminAccount/SignOut" class="logoutbtnstyle btn btn-danger" id="btn">خروج</a>
